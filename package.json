{
	"author": "MTD",
	"bugs": {
		"url": "https://github.com/CUMTD/developer-web/issues"
	},
	"contributors": [
		{
			"name": "Ryan Blackman",
			"url": "https://github.com/rblackman"
		},
		{
			"name": "Matt Novelli",
			"url": "https://github.com/mattnovelli"
		}
	],
	"description": "Monorepo for MTD Developer API docs site and NPM packages",
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@changesets/cli": "^2.29.8",
		"@commitlint/cli": "^20.4.1",
		"@commitlint/config-conventional": "^20.4.1",
		"@types/node": "^25.2.2",
		"cross-env": "^10.1.0",
		"dotenv": "^17.2.4",
		"dotenv-cli": "^11.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"supabase": "^2.76.6",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": "v22.x"
	},
	"homepage": "https://mtd.dev/",
	"license": "Apache-2.0",
	"name": "@mtd.org/developer-web-monorepo",
	"packageManager": "pnpm@10.28.2",
	"private": true,
	"repository": {
		"type": "github",
		"url": "https://github.com/CUMTD/developer-web"
	},
	"scripts": {
		"build": "pnpm -r --workspace-concurrency=1 --sort build",
		"changeset": "changeset",
		"changeset:version": "changeset version && pnpm install --lockfile-only",
		"check": "biome check .",
		"check:fix": "biome check . --write",
		"ci:verify": "pnpm check && pnpm typecheck",
		"clean": "pnpm -r clean",
		"dev": "pnpm -r dev",
		"format": "biome format .",
		"format:fix": "biome format . --write",
		"gen": "pnpm -r gen",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"prebuild": "pnpm run gen",
		"preinstall": "corepack enable",
		"prepare": "husky",
		"release": "pnpm build && changeset publish",
		"start": "pnpm -r start",
		"typecheck": "pnpm -r typecheck",
		"typegen": "pnpm -r typegen"
	},
	"type": "module",
	"version": "3.0.0"
}
