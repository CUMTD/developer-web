		},
		{
			"name": "Matt Novelli",
			"url": "https://github.com/mattnovelli"
		}
	],
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@commitlint/cli": "^20.4.0",
		"@commitlint/config-conventional": "^20.4.0",
		"@types/node": "^25.1.0",
		"cross-env": "^10.1.0",
		"dotenv": "^17.2.3",
		"dotenv-cli": "^11.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": "v22.x"
	},
	"name": "developer-web",
	"packageManager": "pnpm@10.28.2",
	"private": true,
	"repository": {
		"type": "github",
		"url": "https://github.com/CUMTD/developer-web"
	},
	"scripts": {
		"build": "pnpm -r --workspace-concurrency=1 --sort build",
		"check": "biome check .",
		"check:fix": "biome check . --write",
		"ci:verify": "pnpm check && pnpm typecheck",
		"clean": "pnpm -r clean",
		"dev": "pnpm -r dev",
		"format": "biome format .",
		"format:fix": "biome format . --write",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"preinstall": "corepack enable",
		"prepare": "husky",
		"start": "pnpm -r start",
		"typecheck": "pnpm -r typecheck",
		"typegen": "pnpm -r typegen"
	},
	"type": "module",
	"version": "3.0.0"
}
